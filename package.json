{
  "name": "@wyx-ui/react",
  "version": "0.1.1",
  "description": "WYX UI â€” A React UI component library with rich interactions.",
  "keywords": [
    "react",
    "ui",
    "component",
    "components",
    "typescript",
    "scss",
    "animation",
    "wyx-ui"
  ],
  "homepage": "https://github.com/weishimosiji/wyx-ui#readme",
  "bugs": {
    "url": "https://github.com/weishimosiji/wyx-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weishimosiji/wyx-ui.git",
    "directory": "packages/wyx-ui"
  },
  "license": "MIT",
  "author": "weishimosiji",
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "devDependencies": {
    "@types/node": "^24.8.1",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "esbuild-sass-plugin": "^3.3.1",
    "sass": "^1.93.2",
    "tsup": "^7.3.0",
    "typescript": "^5.6.0"
  },
  "scripts": {
    "build": "tsup --format esm,cjs --dts --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "tsup --watch --format esm,cjs --dts"
  }
}
